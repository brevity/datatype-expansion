{
  "name": "datatype-expansion",
  "version": "0.0.1",
  "description": "Utility tool to expand a given RAML type and create a canonical form",
  "main": "src/index.js",
  "scripts": {
    "lint": "standard",
    "test-cov": "istanbul --include-all-sources cover node_modules/mocha/bin/_mocha -- -R dot --recursive --bail test/unit",
    "test-spec": "node_modules/mocha/bin/_mocha -R spec --recursive --bail test/unit",
    "test": "npm run lint && npm run test-cov",
    "test-integration": "node_modules/mocha/bin/_mocha -R spec --recursive --bail test/integration",
    "test-integration-cov": "istanbul --include-all-sources cover node_modules/mocha/bin/_mocha -R spec --recursive --bail test/integration"
  },
  "keywords": [
    "raml",
    "json",
    "types"
  ],
  "author": "Mulesoft",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^4.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "rewire": "^2.5.2",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "yaml-js": "^0.1.3"
  },
  "files": [
    "src/**"
  ]
}
